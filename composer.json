{
    "name": "myparcelcom/magento",
    "description": "Magento 2 extension for MyParcel.com",
    "type": "magento2-module",
    "homepage": "https://www.myparcel.com",
    "keywords": [
        "MyParcel.com",
        "MyParcel",
        "Magento 2",
        "Magento"
    ],
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "ext-json": "*",
        "myparcelcom/api-sdk": "^3.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "MyParcelCOM\\Magento\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
